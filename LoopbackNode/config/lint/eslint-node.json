{
  "env": {
    "es6": false,
    "mocha": true,
    "node": true
  },
  "plugins": [
    "node",
    "mocha"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "node/exports-style": [
      "error",
      "module.exports"
    ],
    "no-unused-vars": 0,
    "node/no-missing-require": 0,
    "node/no-unpublished-require": 0,
    "node/no-unsupported-features": 0,
    "no-inner-declarations": 0,
    "no-constant-condition": 0,
    "no-process-exit": 0,
    "unparam": 0,
    "this": 0,
    "regexp": 0,
    "vars": 0
  },
  "globals": {
    "client": true,
    "country": true,
    "process": true,
    "describe": true,
    "it": true,
    "execute": true,
    "expect": true
  }
}